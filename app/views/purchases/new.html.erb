<h2><%= t :title_send_report %></h2>


<%= form_tag(import_purchases_path, multipart: true) do %>
  <div class="form-group">
    <%= label_tag t :purchases_file %>
    <%= file_field_tag :purchases_file, class: "form-control", required: true %>
  </div>

  <%= submit_tag t(:send_button), class: "btn btn-primary" %>
<% end %>
